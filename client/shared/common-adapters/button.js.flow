import React, {Component} from 'react'

export type Props = {
  onClick: ?Function,
  label: ?string,
  style?: ?Object,
  type: 'Primary' | 'Secondary' | 'Danger' | 'Follow' | 'Following' | 'Unfollow',
  disabled?: ?bool,
  small?: ?bool,
  dz2?: boolean, // Use DZ2 styling?
